<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/register_non_manifest_receiver_button"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="register non manifest receiver"
        android:textSize="12sp" />

    <Button
        android:id="@+id/unregister_non_manifest_receiver_button"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="unregister non manifest receiver"
        android:textSize="12sp" />

    <Button
        android:id="@+id/register_go_async_receiver_button"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="register go async receiver"
        android:textSize="12sp" />

    <Button
        android:id="@+id/unregister_go_async_receiver_button"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="unregister go async receiver"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/go_async_result"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:textSize="12sp"/>

    <Button
        android:id="@+id/register_peek_receiver_button"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="register peek receiver"
        android:textSize="12sp" />

    <Button
        android:id="@+id/unregister_peek_receiver_button"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="unregister peek receiver"
        android:textSize="12sp" />

    <Button
        android:id="@+id/register_nums_receiver_button"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="register nums receiver"
        android:textSize="12sp" />

    <Button
        android:id="@+id/unregister_nums_receiver_button"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="unregister nums receiver"
        android:textSize="12sp" />

    <Button
        android:id="@+id/send_nums_intent_button"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="send nums intent"
        android:textSize="12sp" />

    <androidx.constraintlayout.helper.widget.Flow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:constraint_referenced_ids="register_non_manifest_receiver_button, unregister_non_manifest_receiver_button, register_go_async_receiver_button, unregister_go_async_receiver_button, go_async_result, register_peek_receiver_button, unregister_peek_receiver_button, register_nums_receiver_button, unregister_nums_receiver_button, send_nums_intent_button
" />

</androidx.constraintlayout.widget.ConstraintLayout>